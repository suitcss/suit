{
  "extends": "airbnb/legacy",
  "rules": {
    "vars-on-top": 0,
    "no-console": 0,
    "comma-dangle": [2, "never"],
    "max-len": 0,
    "func-names": 0,
    "one-var": [2, { uninitialized: "always", initialized: "never" }],
    "no-param-reassign": [0],
    "no-use-before-define": [2, "nofunc"],
    "space-before-function-paren": 0,
    "space-infix-ops": 0,
    "object-curly-spacing": 0
  }
}
